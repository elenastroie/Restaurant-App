<Window x:Class="RestaurantOnline.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RestaurantOnline"
        xmlns:views="clr-namespace:RestaurantOnline.Views"
        mc:Ignorable="d"
        Title="RestaurantOnline" Height="600" Width="400"
        ResizeMode="NoResize">
    <Grid>
        <views:SignInView x:Name="SignInView" Visibility="Visible"/>
        <views:SignUpView x:Name="SignUpView" Visibility="Hidden"/>
    </Grid>
</Window>
